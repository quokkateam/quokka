  "dependencies": {
    "immutable": "^3.8.1",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
